build src/test/Tests: cpp_LINKER src/test/c644cb3@@Tests@exe/FigurTest.cpp.o src/test/c644cb3@@Tests@exe/PolynomialTest.cpp.o | /usr/lib/libgtest.a /usr/lib/libgtest_main.a
 LINK_ARGS = -Wl,--as-needed -Wl,--no-undefined -Wl,--start-group -lgtest -lgtest_main -Wl,--end-group -pthread

# Test rules

build meson-test: CUSTOM_COMMAND all PHONY
 COMMAND = /home/romamart/anaconda3/bin/meson test --no-rebuild --print-errorlogs
 DESC = Running$ all$ tests.
 pool = console

build test: phony meson-test

build meson-benchmark: CUSTOM_COMMAND all PHONY
