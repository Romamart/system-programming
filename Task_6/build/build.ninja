ninja_required_version = 1.5.1

# Rules for compiling.

rule cpp_COMPILER
 command = c++ $ARGS -MD -MQ $out -MF '$DEPFILE' -o $out -c $in
 deps = gcc
 depfile = $DEPFILE
 description = Compiling C++ object $out.

# Rules for linking.

rule cpp_LINKER
 command = c++ $ARGS -o $out $in $LINK_ARGS
 description = Linking target $out.

# Other rules

rule CUSTOM_COMMAND
 command = $COMMAND
 description = $DESC
 restat = 1

rule REGENERATE_BUILD
 command = /home/romamart/anaconda3/bin/meson --internal regenerate /home/romamart/CLionProjects/Task_6 /home/romamart/CLionProjects/Task_6/build --backend ninja
 description = Regenerating build files.
 generator = 1

# Phony build target, always out of date

build PHONY: phony 

# Build rules for targets

build src/test/c644cb3@@Tests@exe/FigurTest.cpp.o: cpp_COMPILER ../src/test/FigurTest.cpp
 DEPFILE = src/test/c644cb3@@Tests@exe/FigurTest.cpp.o.d
 ARGS = -Isrc/test/c644cb3@@Tests@exe -Isrc/test -I../src/test -Isrc -I../src -fdiagnostics-color=always -pipe -D_FILE_OFFSET_BITS=64 -Wall -Winvalid-pch -Wnon-virtual-dtor -std=c++17 -g -pthread

build src/test/c644cb3@@Tests@exe/PolynomialTest.cpp.o: cpp_COMPILER ../src/test/PolynomialTest.cpp
 DEPFILE = src/test/c644cb3@@Tests@exe/PolynomialTest.cpp.o.d
